<div class="row">
  <div class="col-auto">
    <h2 class="user-name">Welcome, {{userData.name}}</h2>
  </div>
</div>


<section class="container">
  <h3 class="gallery-name">{{user.name}}</h3>
  <section class="flex-row justify-center">
    <!-- For each painting that belongs to this gallery, the painting-details partial is used to render each painting's data -->
    {{#each userTrips as |trip|}}
    {{> user-trips}}
    {{/each}}
  </section>
  <div>
    <p>
      {{currentCurrency}}
    </p>
  </div>
</section>



<script src="./js/profile.js"></script>
